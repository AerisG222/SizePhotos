{
    "title": "SizePhotos",
    "authors": [ "Mike Morano <mmorano@mikeandwan.us>" ],
    "description": "Tool to prepare photos for my website",
    "version": "0.3.0",
    "packOptions": {
        "licenseUrl": "https://github.com/AerisG222/SizePhotos/blob/master/LICENSE.md",
        "projectUrl": "https://github.com/AerisG222/SizePhotos",
        "summary": "Tool to prepare photos for a website",
        "tags": [ "photography", "photo", "resize", "scale", "raw", "exif" ]
    },
    "buildOptions": {
        "debugType": "portable",
        "emitEntryPoint": true,
        "warningsAsErrors": true,
        "copyToOutput": [ "../../pp3/*.pp3" ]
    },
    "commands": {
        "run": "SizePhotos" 
    },
    "dependencies": {
        "System.CommandLine": "0.1.0-e170103-1",
        "NDCRaw": "0.4.0",
        "NExifTool": "0.6.1",
        "NImgmin": "0.3.0",
        "NMagickWand": "0.5.0",
        "NRawTherapee": "0.2.1"
    },
    "frameworks": {
        "netcoreapp1.1": {
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "version": "1.1.0",
                    "type": "platform"
                }
            },
            "imports": [
                "dotnet55",
                "portable-dnxcore50+net45+win8+wp8+wpa81"
            ]
        }
    }
}
